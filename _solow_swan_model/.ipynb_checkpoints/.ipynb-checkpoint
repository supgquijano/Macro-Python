     "name": "stdout",
     "output_type": "stream",
     "text": [
      "El capital de estado estacionario es: 7.48\n"
     ]
    }
   ],
   "source": [
    "import matplotlib.pyplot as plt\n",
    "\n",
    "def acumulacion_hacia_equilibrio(k0, s, alpha, delta, n, num_periodos):\n",
    "    capitales = [k0]\n",
    "\n",
    "    for _ in range(num_periodos):\n",
    "        k_t = capitales[-1]\n",
    "        # -1 indica todo el capital acumulado en el periodo anterior.\n",
    "        k_t1 = k_t * (1 - n - delta) + s * (k_t ** alpha)\n",
    "        capitales.append(k_t1)\n",
    "\n",
    "    return capitales\n",
    "\n",
    "# Parámetros\n",
    "capital_inicial = 1\n",
    "tasa_ahorro = 0.2\n",
    "parametro_alpha = 0.2\n",
    "tasa_depreciacion = 0.02\n",
    "tasa_crecimiento_poblacional = 0.02\n",
    "num_periodos = 300\n",
    "\n",
    "# Calcula la acumulación de capital per cápita\n",
    "capitales_acumulados = acumulacion_hacia_equilibrio(capital_inicial, tasa_ahorro, parametro_alpha, tasa_depreciacion, tasa_crecimiento_poblacional, num_periodos)\n",
    "\n",
    "# Crear la gráfica\n",
    "plt.figure(figsize=(10, 6))\n",
    "plt.plot(range(num_periodos + 1), capitales_acumulados, marker='o')\n",
    "plt.xlabel('Período')\n",
    "plt.ylabel('Capital per cápita')\n",
    "plt.title('Evolución del Capital per cápita hacia el Equilibrio Estacionario')\n",
    "plt.grid()\n",
    "plt.show()\n",
    "\n",
    "# Calcula el capital de estado estacionario\n",
    "capital_estado_estacionario = (tasa_ahorro / (tasa_depreciacion + tasa_crecimiento_poblacional))**(1 / (1 - parametro_alpha))\n",
    "print(f\"El capital de estado estacionario es: {capital_estado_estacionario:.2f}\")"
   ]
  }
